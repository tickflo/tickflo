    "@react-router/serve": "^7.3.0",
    "cookie": "^1.0.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.0",
    "express": "^4.21.2",
    "isbot": "^5.1.23",
    "morgan": "^1.10.0",
    "pg": "^8.14.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
